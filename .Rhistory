## Generating a formula without one of the explanatory variables
form <- paste(names[1], "~", paste(names[!(names %in% c(names[1], name))],
collapse = " + "))
## Calculating the values for the temporary linear model
temp_lm <- lm(as.formula(form), lm$model[names != name])
temp_summary <- summary(temp_lm)
temp_adjr2 <- temp_summary$adj.r.squared
## If the linear model created above has lower adjusted R-squared value
if (temp_adjr2 > new_adjr2) {
## Replacing the new model with the temporary model
new_adjr2 <- temp_adjr2
new_lm <- temp_lm
}
}
## If a different model has not been found to be better
if (new_adjr2 == adjr2) {
return(lm)
}
## Otherwise, replace the current model with the new one, and try to find a better one, with the same
## method
lm <- new_lm
lm <- lm.optmize.adjr2(lm)
return(lm)
}
